                        <div class="login-box">
                            <h2>ISTE Students Chapter'</h2>
                            <form action="/register" method="POST">
                               
                                    <form>
                                        <div class="user-box">
                                            <input type="text" name="firstName" required="">
                                            <label>First Name</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="text" name="lastName" required="" value="{{surName}}">
                                            <label>Last Name</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="email" class="form-control" id="exampleInputEmail1"
                                                aria-describedby="emailHelp" required="" name="email" required=""
                                                value="{{email}}">
                                            <label>Email</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="text" name="domain" required="">
                                            <label>Domain intreseted</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="text" name="yearOfStudy" required="" min="1" max="2">
                                            <label>Year of Study</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="number" name="mobileNo" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}"
                                                maxlength="12" required="">
                                            <label>Mobile No</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="number" name="registrationNo"
                                                pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" maxlength="12" required="">
                                            <label>Registration No</label>
                                        </div>
                                        <div class="user-box">
                                            <input type="number" name="rollNo" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}"
                                                maxlength="12" required="">
                                            <label>Roll No</label>
                                            <a href="#">
